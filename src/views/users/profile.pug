extends ../base
include ../mixins/video

block content
    div.profile__data
        img(src="/"+user.avatarUrl).avatar
        h3=user.name
    div.video__grid
        each video in user.videos 
            +video(video)
            +video(video)
            +video(video)
            +video(video)
            +video(video)
            +video(video)
        else 
            h1 no video found